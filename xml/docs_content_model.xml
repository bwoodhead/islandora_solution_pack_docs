<content_model name="docs_sp_content_model" xmlns="http://www.islandora.ca" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.islandora.ca http://localhost/islandoracm.xsd">
  <mimetypes>
    <type>application/pdf</type>
    <type>application/msword</type>
  </mimetypes>
  <ingest_rules>
    <rule>
      <applies_to>application/pdf</applies_to>
      <applies_to>application/msword</applies_to>
      <ingest_methods>
        <ingest_method class="DocumentProcessor" dsid="TN" file="DocumentProcessor.php" method="ingest" 
            modified_files_ext="pdf" module="islandora_docs_sp">
        </ingest_method>
      </ingest_methods>
    </rule>
  </ingest_rules>
  <datastreams>
    <datastream dsid="TN">
      <display_method class="DocumentProcessor" file="DocumentProcessor.php" method="displayThumbnail" module="islandora_docs_sp"/>
    </datastream>
    <datastream dsid="OBJ">
      <add_datastream_method class="DocumentProcessor" dsid="TN" file="DocumentProcessor.inc"
        method="createDerivatives" modified_files_ext="jpg" module="islandora_docs_sp">
      </add_datastream_method>
    </datastream>
  </datastreams>
</content_model>
